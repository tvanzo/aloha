<body>
<div id="back">
<div class="container2">
	<div class="left"></div>
	<div class="right">
	<div class="formBox">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
					<span class="login100-form-title p-b-34">
						Account Login
					</span>
  <div class="wrap-input100">
    
    <%= f.email_field :email, autofocus: true, autocomplete: "email", :id => 'first-name', :class=>"input100", placeholder: "Email"%>
  </div>

  <div class="wrap-input100">
    
    <%= f.password_field :password, autocomplete: "off", :class=>'input100', placeholder: "******" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="container-login100-form-btn">
    <%= f.submit 'Sign in', :class => 'login100-form-btn' %>
  </div>
  <%= render "devise/shared/links" %>
  <div class="w-full text-center ">
						
				<%= link_to("Sign Up", "/users/sign_up", :class => 'txt3') %>
				<%= link_to "Sign in with Google", user_google_oauth2_omniauth_authorize_path %>
				</div>
<% end %>
</div>
</div>
</div>
</body>